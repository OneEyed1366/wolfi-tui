{
	"name": "@wolf-tui/css-parser",
	"version": "0.1.0",
	"description": "CSS/SCSS/Less/Stylus parser and code generator for wolf-tui",
	"license": "MIT",
	"type": "module",
	"bin": {
		"wolf-css": "./build/cli.js"
	},
	"exports": {
		".": {
			"types": "./build/index.d.ts",
			"default": "./build/index.js"
		},
		"./vite": {
			"types": "./build/vite.d.ts",
			"default": "./build/vite.js"
		},
		"./esbuild": {
			"types": "./build/esbuild.d.ts",
			"default": "./build/esbuild.js"
		}
	},
	"engines": {
		"node": ">=20"
	},
	"scripts": {
		"dev": "vite build --watch",
		"build": "vite build",
		"test": "vitest run"
	},
	"files": [
		"build"
	],
	"dependencies": {
		"postcss": "^8.4.49"
	},
	"devDependencies": {
		"@types/less": "^3.0.8",
		"@types/stylus": "^0.48.43",
		"esbuild": "^0.24.2",
		"less": "^4.2.1",
		"sass": "^1.83.0",
		"stylus": "^0.64.0",
		"vite": "^6.4.1"
	},
	"peerDependencies": {
		"@wolfie/core": "workspace:*",
		"esbuild": ">=0.17",
		"vite": ">=5.0"
	},
	"peerDependenciesMeta": {
		"@wolfie/core": {
			"optional": false
		},
		"esbuild": {
			"optional": true
		},
		"vite": {
			"optional": true
		}
	}
}

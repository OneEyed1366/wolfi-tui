{
	"name": "@wolfie/angular",
	"version": "0.1.0",
	"description": "Angular adapter for Wolfie",
	"license": "MIT",
	"type": "module",
	"sideEffects": false,
	"exports": {
		".": {
			"types": "./build/index.d.ts",
			"default": "./build/index.js"
		},
		"./styles": {
			"types": "./build/styles/index.d.ts",
			"default": "./build/styles/index.js"
		}
	},
	"engines": {
		"node": ">=20"
	},
	"scripts": {
		"dev": "vite build --watch",
		"build": "vite build",
		"prepare": "npm run build",
		"test": "vitest run",
		"test:watch": "vitest",
		"example": "pnpm --filter @examples/angular-vite example"
	},
	"files": [
		"build"
	],
	"dependencies": {
		"@wolfie/core": "workspace:*",
		"ansi-escapes": "^7.2.0",
		"ansi-styles": "^6.2.1",
		"auto-bind": "^5.0.1",
		"cli-boxes": "^3.0.0",
		"cli-cursor": "^4.0.0",
		"cli-truncate": "^5.1.1",
		"code-excerpt": "^4.0.0",
		"es-toolkit": "^1.39.10",
		"indent-string": "^5.0.0",
		"is-in-ci": "^2.0.0",
		"patch-console": "^2.0.0",
		"signal-exit": "^3.0.7",
		"stack-utils": "^2.0.6",
		"type-fest": "^4.27.0",
		"wrap-ansi": "^9.0.0"
	},
	"devDependencies": {
		"@angular/common": "^19.0.0",
		"@angular/compiler": "^19.0.0",
		"@angular/core": "^19.0.0",
		"@wolfie/css-parser": "workspace:*",
		"@wolfie/plugin": "workspace:*",
		"memfs": "^4.56.10",
		"rxjs": "^7.8.0",
		"ts-loader": "^9.5.4"
	},
	"peerDependencies": {
		"@angular/core": ">=17.0.0",
		"@angular/common": ">=17.0.0",
		"chalk": "^5.0.0"
	}
}
